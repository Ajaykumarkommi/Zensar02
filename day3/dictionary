for key,value in d.items():
    print(key,value)
print("___prob4___"*1)
n=5
dict=dict()
for i in range(1,n+1):
    dict[i]=i*i
print(dict)
print("___prob5___"*1)
def removeDuplicates(dic):

    temp = []

    res = {}

    for key, val in dic.items():

        if val not in temp:

            temp.append(val)

            res[key] =  val

    return res
print(removeDuplicates({"a":10,"b":20,"c":30,"d":20}))
