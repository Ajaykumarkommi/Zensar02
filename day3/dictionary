print("___prob2___"*1)
d1={1:10,2:20,3:30,4:40,5:50,6:60}
def is_presented(x):
    if x in d1:
        print("key present")
    else:
       print("key not present")
is_presented(1)
is_presented(4)
is_presented(9)
